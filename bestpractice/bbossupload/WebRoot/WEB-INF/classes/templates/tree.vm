			      	linkUrl += "&selectedNode=" + selectNode; 
			      }
			      
			      var indent = eventNode_.attr("indent");
			      
			      linkUrl += "&node_parent_indent=" + indent;
			      linkUrl += "&__nodename=" + name;
			      linkUrl += "&__nodetype=" + nodetype;
			      if(nodepath != null && nodepath != 'null' && nodepath != "")
