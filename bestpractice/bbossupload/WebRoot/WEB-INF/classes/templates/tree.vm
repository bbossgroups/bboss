      function doClickImageIcon(linkUrl,eventNode)
      {

	      if(eventNode)
	      {
	      	eventNode = convertValue(eventNode,true);
	          var eventNode_ = $("#icon_" + eventNode);
	      	  if(eventNode_ == null)
	      	  	return ;
