		      		nodetoggle(eventNode,false); 
		      }
		      else 
		      { 
			      if(selectNode) 
			      {
			      	linkUrl += "&selectedNode=" + selectNode; 
			      }
			      
			      var indent = eventNode_.indent;
			      linkUrl += "&node_parent_indent=" + indent;
