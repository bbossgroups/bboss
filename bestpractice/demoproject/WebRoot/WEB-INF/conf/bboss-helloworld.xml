<properties>
	<property name="/examples/*.page"
		path:sayHello="/examples/hello.jsp"
		class="org.frameworkset.mvc.HelloWord">
	</property>
	<property name="/file/*.page"
			  path:fileupload="/examples/fileupload.jsp"
			  path:foldertree="/examples/foldertree.jsp"
			  path:filelist="/examples/filelist.jsp"
			  path:uptable="/examples/uptable.jsp"
			  f:rootPath="attr:file.workfolder"
			  f:filedomain="attr:file.domain"
			  class="org.frameworkset.mvc.FileController">
	</property>
	<property name="file.domain" value="http://bpitresource.sany.com.cn/"/>
	<!--<property name="file.workfolder" value="/data/bpit/www"/>-->
	<property name="file.workfolder" value="d:/upload"/>
</properties>